#include <iostream>
#include <cstdlib> //biblioteki
#include <cmath>
#include <iomanip>

using namespace std;

int main(){
	
	int a, b, c;
	
	cout<<"Podaj a: ";
	cin>>a>>endl;
	cout<<"Podaj b: ";
	cin>>b>>endl;
	
	if(b=0){
	{
		cout<<"wynik: "<<a<<endl;
		system("pause");
	}
	else
		{
			while(b!=0)
				{
					c = a%b;
					a=b;
					b=c;
				}
				cout<<"Wynik: "<<a<<endl;
		}
}
		
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
